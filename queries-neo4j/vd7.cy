MATCH (sgLabel)<-[:PREFLABEL]-(sg:StudyGroup)<-[:INGROUP]-(s:Study)-[:INSTRUMENT]->(qn:Questionnaire)-[:QUESTION]->(q)-[:QUESTIONTEXT]->(qt), (qn)-[:DESCRIPTION]->(desc), (s)-[:PRODUCT]->(:LogicalDataSet)-[:PREFLABEL]->(datasetLabel), (s)-[:PREFLABEL]->(sLabel), (qn)-[:COUNTRY]->(country) WHERE sgLabel.en = 'EU-LFS' AND sLabel.en = '2008' AND datasetLabel.en = 'yearly' AND country.code = 'UK' RETURN DISTINCT desc.en, qt.en;