MATCH (studyLabel)<-[:PREFLABEL]-(s:Study)-[:INGROUP]->(:StudyGroup)-[:PREFLABEL]->(studyGroupLabel), (s)-[:PRODUCT]->(lds:LogicalDataSet)-[:PREFLABEL]->(datasetLabel), (lds)-[:VARIABLE]->(variable)<-[:VARIABLE]-(ss:SummaryStatistics)-[:SPATIAL]->(country) WHERE studyGroupLabel.en = 'EU-LFS' AND studyLabel.en = '1996' AND datasetLabel.en = 'yearly' AND variable.notation = 'STAPRO' AND country.label = 'PT' RETURN ss.numberOfCases, ss.numberOfCasesMissingOnly, ss.mean, ss.standardDeviation;