PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX disco: <http://rdf-vocabulary.ddialliance.org/discovery#> PREFIX missy: <http://lod.gesis.org/missy#> PREFIX gn: <http://www.geonames.org/ontology#> PREFIX dcterms: <http://purl.org/dc/terms/> PREFIX skos: <http://www.w3.org/2004/02/skos/core#> SELECT ?questiontext WHERE {?study a disco:Study ; skos:prefLabel ?studyLabel ; disco:inGroup [ a disco:StudyGroup ; skos:prefLabel ?studyGroupLabel ] ; disco:instrument [a disco:Questionnaire ; dcterms:description ?description ; disco:question [a disco:Question ; disco:questionText ?questiontext ] ; missy:country [ gn:countryCode ?countryLabel ] ]. FILTER ( ?studyGroupLabel = 'EU-SILC'@en ) FILTER ( ?studyLabel = '2005'@en ) FILTER ( ?countryLabel = 'UK' ) FILTER ( ?description = 'basic questionnaire'@en )}
