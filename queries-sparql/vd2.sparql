PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX disco: <http://rdf-vocabulary.ddialliance.org/discovery#> PREFIX dcterms: <http://purl.org/dc/terms/> PREFIX skos: <http://www.w3.org/2004/02/skos/core#> SELECT DISTINCT ?variablename WHERE { ?study a disco:Study ; skos:prefLabel ?studyLabel ; disco:product [ a disco:LogicalDataSet ; skos:prefLabel ?datasetLabel ; disco:containsVariable [ a disco:Variable ; skos:notation ?variablename ] ] ; disco:inGroup [ a disco:StudyGroup ; skos:prefLabel ?studyGroupLabel ] . FILTER ( ?studyGroupLabel = 'CIS'@en ) FILTER ( ?studyLabel = '3'@en ) FILTER ( ?datasetLabel = 'cross-sec'@en ) }
