PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX disco: <http://rdf-vocabulary.ddialliance.org/discovery#>PREFIX missy: <http://lod.gesis.org/missy#> PREFIX dcterms: <http://purl.org/dc/terms/> PREFIX gn: <http://www.geonames.org/ontology#> PREFIX skos: <http://www.w3.org/2004/02/skos/core#> SELECT DISTINCT ?description ?questiontext WHERE { ?study a disco:Study ; skos:prefLabel ?studyLabel ; disco:inGroup [ a disco:StudyGroup ; skos:prefLabel ?studyGroupLabel ] ; disco:product [ a disco:LogicalDataSet ; skos:prefLabel ?datasetLabel ] ; disco:instrument [ a disco:Questionnaire ; dcterms:description ?description ; missy:country [ a missy:Country ; gn:countryCode ?countryLabel ] ; disco:question [ a disco:Question ; disco:questionText ?questiontext ] ] . FILTER ( ?studyGroupLabel = 'EU-LFS'@en ) FILTER ( ?studyLabel = '2008'@en ) FILTER ( ?datasetLabel = 'yearly'@en ) FILTER ( ?countryLabel = 'UK' ) }
