PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX disco: <http://rdf-vocabulary.ddialliance.org/discovery#> PREFIX missy: <http://lod.gesis.org/missy#> PREFIX dcterms: <http://purl.org/dc/terms/> PREFIX skos: <http://www.w3.org/2004/02/skos/core#> SELECT ?noOfCases ?noOfCasesMO ?mean ?standardDeviation WHERE { ?study a disco:Study ; skos:prefLabel ?studyLabel ; disco:inGroup [ a disco:StudyGroup ; skos:prefLabel ?studyGroupLabel ] ; disco:product [a disco:LogicalDataSet ; skos:prefLabel ?datasetLabel ; disco:containsVariable ?variable ] . ?variable a disco:Variable ; skos:notation ?variablenotation . FILTER ( ?studyGroupLabel = 'EU-LFS'@en ) FILTER ( ?studyLabel = '1996'@en ) FILTER ( ?datasetLabel = 'yearly'@en ) FILTER ( ?variablenotation = 'STAPRO' ) { ?statistics a disco:SummaryStatistics ; disco:statisticsVariable ?variable ; dcterms:spatial [ a dcterms:Location ; dcterms:location ?countrylabel ] ; disco:numberOfCases ?noOfCases ; missy:numberOfCasesMissingOnly ?noOfCasesMO ; disco:mean ?mean ; disco:standardDeviation ?standardDeviation FILTER ( ?countrylabel = 'PT' ) } }
