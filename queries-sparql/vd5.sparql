PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX disco: <http://rdf-vocabulary.ddialliance.org/discovery#> PREFIX dcterms: <http://purl.org/dc/terms/> PREFIX skos: <http://www.w3.org/2004/02/skos/core#> SELECT DISTINCT ?studyLabel ?questiontext WHERE { ?study a disco:Study ; skos:prefLabel ?studyLabel ; disco:inGroup [ a disco:StudyGroup ; skos:prefLabel ?studyGroupLabel ] ; disco:product [ a disco:LogicalDataSet ; skos:prefLabel ?datasetLabel ; disco:containsVariable ?variable ] . ?variable a disco:Variable ; skos:notation ?variablename ; disco:question [ a disco:Question ; disco:questionText ?questiontext ] . FILTER ( ?studyGroupLabel = 'CIS'@en ) FILTER ( ?studyLabel = '2008'@en ) FILTER ( ?variablename = 'RDENG' )}
