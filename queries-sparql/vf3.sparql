PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX disco: <http://rdf-vocabulary.ddialliance.org/discovery#> PREFIX dcterms: <http://purl.org/dc/terms/> PREFIX skos: <http://www.w3.org/2004/02/skos/core#> SELECT ?countryLabel ?conceptnotation ?conceptlabel ?frequency ?percentage WHERE { ?study a disco:Study ; skos:prefLabel ?studyLabel ; disco:inGroup [ a disco:StudyGroup ; skos:prefLabel ?studyGroupLabel ] ; disco:product [ a disco:LogicalDataSet ; skos:prefLabel ?datasetLabel ; disco:containsVariable [a disco:Variable ; skos:notation ?variablenotation ; disco:representation ?conceptScheme ] ] .{?conceptScheme a skos:ConceptScheme ; skos:hasTopConcept ?concept .?concept skos:notation ?conceptnotation ; skos:prefLabel ?conceptlabel .?statistics a disco:CategoryStatistics ; dcterms:spatial [a dcterms:Location ; dcterms:location ?countryLabel ] ; disco:statisticsCategory ?concept ; disco:frequency ?frequency ; disco:percentage ?percentage .} FILTER ( ?studyGroupLabel = 'CIS'@en ) FILTER ( ?studyLabel = '2008'@en ) FILTER ( ?datasetLabel = 'cross-sec'@en ) FILTER ( ?variablenotation = 'LARMAR' ) FILTER ( ?countryLabel = 'LV' )} ORDER BY ?conceptnotation
